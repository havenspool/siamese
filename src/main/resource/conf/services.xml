<?xml version="1.0" encoding="UTF-8" ?>

<server>
    <instances>
        <instance>com.havens.siamese.service.StartupService</instance>
    </instances>

    <services>
        <service code="time_check">com.havens.siamese.service.TimeCheckService</service>
        <service code="user_login">com.havens.siamese.service.UserLoginService</service>
        <service code="get_roles">com.havens.siamese.service.GetRolesService</service>
        <service code="new_role">com.havens.siamese.service.NewRolesService</service>

        <!-- room -->
        <service code="enter_room">com.havens.siamese.service.room.EnterRoomService</service>
        <service code="exit_room">com.havens.siamese.service.room.ExitRoomService</service>

        <!-- desk -->
        <service code="change_desk">com.havens.siamese.service.desk.ChangeDeskService</service>
        <service code="desk_info">com.havens.siamese.service.desk.DeskInfoService</service>

        <!-- card -->
        <service code="do_banker">com.havens.siamese.service.card.DoBankerService</service>
        <service code="get_banker">com.havens.siamese.service.card.GetBankerService</service>
        <service code="bet">com.havens.siamese.service.card.BetService</service>
